/* product LIST view styles */
.discount-label {
	background: url('images/discount_label_left.png') no-repeat left top;
	padding: 0 0 0 10px;
}
.discount-label em {
	background: #37aae8 url('images/discount_label_right.png') no-repeat right top;
	padding: 4px 4px 4px 1px;
