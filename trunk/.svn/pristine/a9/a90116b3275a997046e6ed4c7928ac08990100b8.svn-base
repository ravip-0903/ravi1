	height: 16px;
}
.discount-label em strong {
	font-weight: normal;
}
.product-config-header {
	font-size: 100%;
	text-align: center;
	padding-top: 20px;
	clear: both;
}
.product-list-price {
	padding-bottom: 4px;
}
