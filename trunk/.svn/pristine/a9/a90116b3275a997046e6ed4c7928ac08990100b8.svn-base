/*--Gallery Main Container--*/
.b_mng{ width:591px; height:195px; display:block; border:1px solid #E3E3E3;}
.anythingSlider                         { width:591px; height:195px; position:absolute;}
.anythingSlider .wrapper                { width:591px; overflow: auto; height:195px;position: absolute;}
.anythingSlider .wrapper ul             { width:9999px; list-style: none; position: absolute; height: 234px; left:-20px;/*modify left accourding to your reqirment*/}
.anythingSlider ul li                   { display: block; float: left; height: 234px; padding-top:0px;}
.anythingSlider ul li img               { margin-left:0px;}
.anythingSlider .arrow                  { display: block; height: 70px; width: 67px; background: url(../images/arrows.png) no-repeat 0 0; text-indent: -9999px; position: absolute; top: 65px; cursor: pointer; }
.anythingSlider .back                { z-index:2; right:825px; top:92px;	width:70px;	background:url(../images/gallery_left_bt.png) no-repeat ; display:none; }
.anythingSlider .forward                   { right:13px; top:92px;	width:70px;	background:url(../images/gallery_right_bt.png) no-repeat ; display:none;}


.vendor_review_count{ /*--float:left;--*/ margin:7px 10px; color:#007AC0;}


/*--fadein Gallery Main Container--*/
#slideshow {
	width:591px;
	position:relative;
    height:195px;
	overflow:hidden;}
#slideshow IMG {
	width:591px;
	height:195px;
    position:absolute;
    top:0;
    left:0;
    z-index:8;
    opacity:0.0;}
#slideshow IMG.active {
    z-index:10;
    opacity:1.0;}
#slideshow IMG.last-active {
    z-index:9;}

/*--fadein Gallery Main Container--*/


