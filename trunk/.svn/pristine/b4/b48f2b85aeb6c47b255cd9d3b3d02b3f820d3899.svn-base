html,body{height:100%;-webkit-tap-highlight-color:rgba(0,0,0,0);font-family:Slackey}
#viewport{float:left;clear: both; height:466px; width:420px; background:url(http://cdn.shopclues.com/images/banners/fb_contest_bg_roller_spin.png) 7px 9px  no-repeat; margin-left:30px;}
#container h1{font-size:large;text-align:center;color:#FFF}
#container #header{text-align:center}
#container #header h1{text-align:center;font:26px Slackey;text-shadow:#000 0 0 1px , #000 0 0 1px;color:#048ccc}
#container #header h3{color:#555;padding-bottom:10px;text-shadow:rgba(255,255,255,0.6) 0 1px 0}
#container #buttons{margin-top:3px}
#container #buttons #play{width:300px;float:left;font-family:Slackey;text-align:center;height:40px;line-height:40px;font-size:24px;margin:96px 0 0 150px}
#container #reels{display:block; position:relative; height:155px; width:300px; float:left; overflow:hidden; text-align:center; margin:173px 0 0 30px;}
#container #reels canvas{width:66px;height:300px;position:relative;background:#FFF;border:2px solid #FFD700; margin-right:17px;}
#container #overlay{width:100%;height:100%;position:absolute;top:0;opacity:0.3;background-image:0 color-stop(0.5,white), to(grey));display:none}
#container #overlay #winline{width:100%;height:5px;background:red;position:relative;top:50%}
#reels #results{display:none;background: #fff; border: 3px solid #ffd700; height: 127px; width: 240px; left: 13px; top: 0px; position: absolute; font-size: large; line-height: 25px; padding: 10px;}
#results #score{margin:5px}
#results #score img{vertical-align:middle}
.button{position:relative;display:block;text-align:center;color:#fff;box-shadow:inset rgba(255,255,255,0.15) 0 1px 0;border:1px solid rgba(0,0,0,0.15);background-image:0 to(rgba(0,0,0,0.15)));background-position:0 -1px;text-shadow:#000 0 0 1px;text-decoration:none}
.button.button-default{background-image:0 to(#3f93ac));filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#77e0fb',endColorstr='#3f93ac');zoom:1}
.button.button-default:hover{filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffd539',endColorstr='#af861e');zoom:1;background-image:0 to(#af861e));cursor:pointer}
.button.button-default:active{background-image:0 to(#b3671a))}
.winner_blk_cont{width:240px;float:left; border-radius:5px; moz-border-radius:5px; margin:0px 82px 40px; padding:5px 0; background:#fff; border-top-left-radius: 0; border-top-right-radius: 0; moz-border-top-left-radius: 0; moz-border-top-right-radius: 0;}
.winner_info{font-size:12px;text-align:center;color:#666}
.winner_blk{font-size:40px;padding-top:5px;line-height:30px;color:#d33333;font-weight:700;text-align:center;}
.fb_pro_pic{float:left;margin-right:5px}
.post_list{float:left;font:13px/16px trebuchet ms, tahoma, arial;clear:both;border-bottom:1px solid #ccc;margin:0 2%; width:96%; background:#eeecec; position:relative;}
.post_list:hover{background:#e4e1e1}
.date_info{clear:both}
.info_blk{float:left}
.profile_pic{border:3px solid #e8e8e8;margin:3px}
.slot_machine_bg{background:url(smiley/slot_machine_bg.jpg) no-repeat 0 0;background-size:400px auto;width:400px;height:600px;float:left;margin-right:60px;position:relative}
.slot_machine_bg .pic_container{width:210px;float:left;margin:190px 0 0 110px}
.slot_machine_bg img{width:35px;height:35px;margin-left:10px}
.slot_machine_bg .input_btn{position:absolute;top:358px;left:144px}
.slot_machine_bg .winner_blk_new{background-color:rgba(255,255,255,0.5);border:3px solid #FFD700;height:60px;width:320px;left:33px;top:212px;position:absolute;font-size:x-large;padding:5px}
.winner_click{clear:both;float:left}
html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{border:0;outline:0;font-weight:inherit;font-style:inherit;font-size:100%;font-family:inherit;vertical-align:baseline;margin:0;padding:0}
:focus{outline:0}
@font-face{font-family:Slackey;font-style:normal;font-weight:400;src:local(Slackey), url(http://themes.googleusercontent.com/static/fonts/slackey/v3/5wB_ThMKcqfCDp4KQYE4yg.woff) format(woff)}
body{line-height:1;color:#000;background:#FFF;font-family:Lucida Grande,trebuchet ms,verdana,arial,helvetica,sans-serif}
ol,ul{list-style:none}
table{border-collapse:separate;border-spacing:0}
caption,th,td{text-align:left;font-weight:400}
blockquote:before,blockquote:after,q:before,q:after{content:""}
.play_game{float: left; clear: none; margin-left: 10px; cursor:pointer; margin-top:66px;}
#buttons{font-family:Slackey; clear: both; color: #fff; margin:87px 30px 30px 30px; float: left; text-align: center; width: 65%; font-size: 18px;}
#buttons #play2{width: 100%;}
.winner_blk_heading{background:url(http://cdn.shopclues.com/images/banners/winner_blk_heading.png) center center no-repeat; height: 84px; width: 400px; float: left; margin-top: 20px;}
.winner_info{color:#fff; margin-top: 59px;}
.fb_game_block{background: #dee6e9; float: left; padding: 20px 0 20px 20px;}
.name_field{font-size: 15px; margin-top: 6px; color:#083330;}
.name_field_winner{width:20px; text-align:center; height:20px; border-radius:20px; -moz-border-radius:20px; position:absolute; right:-10px; top:10px; background:#f0ac00; font-size:16px; padding:6px; line-height:20px;}
.name_field_time{color:#7a7e81;}