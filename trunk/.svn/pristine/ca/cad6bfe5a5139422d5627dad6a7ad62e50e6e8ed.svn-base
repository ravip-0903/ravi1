/*Header*/
.box_main
{
	width:600px;
	margin:auto;
	background:#fff url(../images/bg_maincontainer.gif) left bottom repeat-x;
}

.logo_shopclues
{
	float:left;
	display:inline;
	width:100%;
	border-bottom:5px solid #eee;
	padding-bottom:5px;
}
.box_heading
{
	float:left;
	display:inline;
	width:100%;
	font:20px trebuchet ms;
	color:#00a0e5;
	margin-top:10px;
}
.box_context
{
	float:left;
	display:inline;
	width:100%;
	font:13px trebuchet ms;
	color:#797c7d;
	line-height:20px;
}
.form_signup
{
	float:left;
	display:inline;
	width:600px;
	border:1px solid #c6c9cc;
	margin-top:20px;
	border-radius:10px;
	-moz-border-radius:10px;
	-webkit-border-radius:10px;
	padding-top:10px;
}
.form_signup .form_signup_row
{
	float:left;
	display:inline;
	width:96%;
	border-bottom:1px solid #eee;
	margin-top:10px;
	margin-left:10px;
	padding-bottom:10px;
}
.form_signup .form_signup_row .form_signup_row_fieldname
{
	float:left;
	display:inline;
	width:39%;
	font:13px trebuchet ms;
	color:#797c7d;
	text-align:right;
	margin-top:5px;
}
.form_signup .form_signup_row .form_signup_row_field
{
	float:left;
	display:inline;
	width:56%;
	margin-left:10px;
}
.form_signup .form_signup_row .form_signup_row_field .form_signup_row_field_textbox
{
	float:left;
	display:inline;
	width:150px;
	border:1px solid #c6c9cc;
	border-radius:5px;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	font:13px trebuchet ms;
	padding:5px;
	color:#797c7d;
}
.form_signup .form_signup_row .form_signup_row_field .form_signup_row_field_textbox:focus
{
	float:left;
	display:inline;
	width:150px;
	border:1px solid #4cb2ff;
	border-radius:5px;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	font:13px trebuchet ms;
	padding:5px;
	color:#313233;
}
.form_signup .form_signup_row .form_signup_row_field .form_signup_row_field_button
{
	float:right;
	display:inline;
	border-radius:3px;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	font:bold 10px verdana;
	padding:5px 10px;
	overflow:hidden;
	color:#fff;
	text-transform:uppercase;
	background:#d34a20;
}
.form_signup .form_signup_row .form_signup_row_field_link
{
	float:left;
	display:inline;
	font:11px verdana;
	margin-left:10px;
	margin-top:2px;
	color:#7c7e80;
	text-decoration:none;
}
.form_signup .form_signup_row .form_signup_row_field .form_signup_row_field_label
{
	float:left;
	display:inline;
	font:12px trebuchet ms;
	margin-left:5px;
	color:#00a0e5;
	text-decoration:none;
}

.box_disclaimer
{
	float:left;
	display:inline;
	width:100%;
	font:11px Trebuchet MS;
	margin-top:20px;
	padding-top:5px;
	border-top:5px solid #eee;
	color: #adb0b3;
}