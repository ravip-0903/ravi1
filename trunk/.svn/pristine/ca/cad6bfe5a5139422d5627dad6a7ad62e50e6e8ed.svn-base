/********** Universal Css **********/
* {
    margin: 0;
    padding: 0;
}
html, body {
    height: 100%;
}

body {
    color: #333333;
    font-family: Arial;
    font-size: 13px;
	background:url(../images/bg_body.gif) left top repeat;
	behavior:url("csshover3.htc");
}
.body_home {
    color: #333333;
    font-family: Arial;
    font-size: 13px;
	background:url(../images/bg_home.jpg) center top no-repeat;
}

img, div, input
{
	behavior: url("iepngfix.htc");
}

div, p, img, ul, ol, li, h1, h2, h3, h4, h5, h6, textarea, select, option, input {
    background-position: left top;
    background-repeat: no-repeat;
    border-width: 0;
    display: block;
    margin: 0;
    padding: 0;
}
.clearboth {
    clear: both;
    font-size: 0;
}
.border {
    border: 1px solid #000000;
}

/********** Universal LINK Css **********/
a {
    color: #333333;
    text-decoration: none;
}
a:hover {
    color: #333333;
    text-decoration: underline;
}

/********** Text Style Css **********/
.bold {
    font-weight: bold;
}
.italic {
    font-style: italic;
}

/********** Floating Css **********/
.float_left {
    display: inline;
    float: left;
}
.float_right {
    display: inline;
    float: right;
}

