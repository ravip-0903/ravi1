            });

            $(".header .topBar .accnt-icn").click(function(e){
                e.stopPropagation();
                $(".cat-menu").hide();
                $(".accnt-option-menu").toggle();
                $(".arrow-up").toggle();
            });
            $(".header .topBar .menu-icn").click(function(e){
                e.stopPropagation();
                $(".accnt-option-menu").hide();
                $(".arrow-up").hide();
                $(".cat-menu").toggle();
            });

            loginAndCartInfo();

            function loginAndCartInfo(){
                var userNameCookie = ReadCookie("scun");
                var cartCookie = ReadCookie("sess_id1");
                // handle login information here with the help of cookies
                if(userNameCookie != ""){
                    var userName = JSON.parse(atob(userNameCookie));
                    var content = '<li><a href="/myaccount">My Account<label class="float_right">Welcome <span class="bold">'+userName+'</span></label></a></li><hr><li><a href="/index.php?dispatch=auth.logout&redirect_url=index.php">Sign Out</a></li>';
                    $("ul.accnt-option-menu").find("li").remove();
                    $("ul.accnt-option-menu").find("hr").remove();
                    $("ul.accnt-option-menu").append(content);
                    $(".header .topBar .accnt-icn").addClass("accnt-icn-signedIn");
                }
                if(cartCookie != ""){
                    var cartContent = JSON.parse(atob(ReadCookie("sess_id1")));
                    var count = 0;
                    for(var key in cartContent){
                        count = count + 1;
                    }
                    $(".header .topBar .cart-content").html(count);
                    $(".header .topBar .cart-content").show();
                }
                else{
                    $(".header .topBar .cart-content").hide();
                }
            }
        });
